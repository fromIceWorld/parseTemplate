    "license": "ISC",
    "bugs": {
        "url": "https://github.com/fromIceWorld/parseHTML/issues"
    },
    "homepage": "https://github.com/fromIceWorld/parseHTML#readme",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^13.2.1",
        "@rollup/plugin-typescript": "^8.3.2",
        "jest": "^27.5.1",
        "tslib": "^2.3.1",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "tern": "^0.24.3"
    }
}
