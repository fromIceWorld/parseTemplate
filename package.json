{
    "name": "parse-html-template",
    "version": "0.0.9",
    "description": "解析HTML,生成层级树。",
    "main": "./dist/index.js",
    "types": "/dist/@types/index.d.ts",
    "files": [
        "dist/*"
    ],
    "scripts": {
        "test": "jest",
        "build": "rollup --config",
        "tsc": "tsc index.ts --outDir ./dist",
